{
  "name": "aws-credful",
  "version": "1.0.13",
  "description": "CLI for obtaining AWS temporary credentials from a SAML browser login",
  "main": "src/index.js",
  "bin": {
    "aws-credful": "run.js"
  },
  "scripts": {
    "start": "electron src/index.js",
    "test": "nyc ava && semistandard",
    "test-only": "nyc ava",
    "watch": "ava --watch",
    "lint": "semistandard",
    "lint-fix": "semistandard --fix"
  },
  "keywords": [
    "AWS",
    "Amazon",
    "SAML",
    "SSO",
    "ADFS",
    "G Suite",
    "Okta",
    "Auth0",
    "Duo",
    "Shibboleth"
