    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "ava": "^3.8.2",
    "eslint-config-standard": "^13.0.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "semistandard": "^14.2.0",
