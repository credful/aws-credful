{
  "name": "aws-credful",
  "version": "1.1.0",
  "description": "CLI for obtaining AWS temporary credentials from a SAML browser login",
  "main": "src/index.js",
  "bin": {
    "aws-credful": "run.js"
  },
  "scripts": {
    "start": "electron src/index.js",
    "test": "nyc ava && semistandard",
    "test-only": "nyc ava",
    "watch": "ava --watch",
    "lint": "semistandard",
    "lint-fix": "semistandard --fix"
  },
  "keywords": [
    "AWS",
    "Amazon",
    "SAML",
    "SAML2",
    "SSO",
    "ADFS",
    "G Suite",
    "Jumpcloud",
    "OneLogin",
    "Okta",
    "Auth0",
    "Duo",
    "Shibboleth"
  ],
  "author": "Jason Stitt",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.398.0",
    "@aws-sdk/shared-ini-file-loader": "^3.370.0",
    "bluebird": "^3.7.2",
    "electron": "^26.1.0",
    "home-dir": "^1.0.0",
    "ini": "^4.1.1",
    "xml2js": "^0.6.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "eslint-config-standard": "^17.1.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "semistandard": "^17.0.0",
    "sinon": "^15.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/credful/aws-credful.git"
  },
  "homepage": "https://github.com/credful/aws-credful#readme",
  "nyc": {
    "check-coverage": true,
    "lines": 100
  }
}
