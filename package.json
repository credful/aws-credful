  "author": "Jason Stitt",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "aws-sdk": "^2.839.0",
    "bluebird": "^3.7.2",
    "electron": "^11.2.3",
    "home-dir": "^1.0.0",
    "ini": "^2.0.0",
    "xml2js": "^0.4.23",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "eslint-config-standard": "^16.0.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "semistandard": "^16.0.0",
    "sinon": "^9.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/credful/aws-credful.git"
  },
  "homepage": "https://github.com/credful/aws-credful#readme",
  "nyc": {
    "check-coverage": true,
